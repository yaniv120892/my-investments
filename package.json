{
  "name": "my-investments",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate --schema src/prisma/schema.prisma && next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate --schema src/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema src/prisma/schema.prisma",
    "db:deploy": "prisma migrate deploy --schema src/prisma/schema.prisma",
    "db:studio": "prisma studio --schema src/prisma/schema.prisma",
    "setup": "npm run db:generate && npm run db:migrate",
    "postinstall": "prisma generate --schema src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@tanstack/react-query": "^5.84.1",
    "@tanstack/react-query-devtools": "^5.84.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^6.4.17",
    "@upstash/redis": "^1.35.3",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "jsonwebtoken": "^9.0.2",
    "next": "15.4.5",
    "nodemailer": "^7.0.5",
    "prisma": "^6.13.0",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "redis": "^5.7.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
